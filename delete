using TMPro;
using UnityEngine;

public class DeleteKey : MonoBehaviour
{
    public TMP_Text output;   // Drag your output text here in Inspector

    public void DeleteCharacter()
    {
        if (output != null && output.text.Length > 0)
        {
            output.text = output.text.Substring(0, output.text.Length - 1);
        }
    }
}
